{
  "version": 2,
  "builds": [
    {
      "src": "bin/server.ts",
      "use": "@vercel/npm",
      "config": {
        "includeFiles": ["**"],
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "bin/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ]
}
